class Hdfc
{
	int amountBal;
	// parametrizd constructors
	
	protected Hdfc(int amountBal){
		this.amountBal=amountBal;
		System.out.println("Welcome to HDFC Bank....");
		
		if(amountBal >= 10000){
			System.out.println("Your account is created and amount deposited successfully ");
		}
		else{
			System.out.println("please enter the valid amount...");
		}
		
	}
	
	void getAmountBalInfo(){
		System.out.println("Your deposited balance is "+ amountBal);
	}
}
class Example9{
	
	// can we make static constructor 
	
	public static void main(String[] args){
		System.out.println("Starts");
		Hdfc cust1= new Hdfc(10000);
		cust1.getAmountBalInfo();
		System.out.println("Ends");
		
	}
}